# NavSwap AI Prediction Microservice Environment Variables
# Copy this file to .env and fill in your actual values

# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Model Paths (Optional - uses default paths if not set)
MODEL_DIR=app/models
QUEUE_MODEL_PATH=app/models/xgb_queue_tuned_model.pkl
WAIT_MODEL_PATH=app/models/xgb_wait_tuned_model.pkl
FAULT_MODEL_PATH=app/models/lgbm_fault_tuned_model.pkl
ACTION_MODEL_PATH=app/models/xgb_action_tuned_model.pkl
SCALER_PATH=app/models/scaler.pkl
FEATURES_PATH=app/models/feature_columns.pkl
ENCODER_PATH=app/models/label_encoder.pkl

# Logging Configuration
LOG_LEVEL=INFO

# CORS Configuration (Optional)
ALLOWED_ORIGINS=*

# Production Settings (Optional)
DEBUG=False
ENVIRONMENT=development